function init()
	pStart = mgGetBool("start", true)
	pEnd = mgGetBool("end", true)
	
	mgFogColor(0.82, 0.79, 0.93, 0.6, 0.5, 0.9)
	mgMusic("5")
	
	confSegment("basic/16_0", 1)
	confSegment("basic/16_1", 1)
	confSegment("basic/16_2", 1)
	confSegment("basic/16_3", 1)
	
	l = 0
	
	local len = 200
	
	while l < len do
		s = nextSegment()
		l = l + mgSegment(s, -l)
	end
	
	if pEnd then
		l = l + mgSegment("basic/door", -l)
	end
	
	mgLength(l)
end

function tick()
end
